{
  "name": "pystudio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.18",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "brace": "^0.11.1",
    "electron-is-dev": "^1.1.0",
    "grommet": "^2.9.0",
    "grommet-icons": "^4.4.0",
    "react": "^16.12.0",
    "react-ace": "^8.0.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "styled-components": "^4.4.1",
    "typeface-roboto": "0.0.75",
    "typescript": "^3.7.3",
    "zeromq": "^6.0.0-beta.5",
    "zmq_jupyter": "^1.1.2"
  },
  "build": {
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ]
  },
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "tsc ./src/electron/*.ts && concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "predist": "npm run build && mkdir build/src && tsc src/electron/*.ts --outDir build/src && tsc src/constants/*.ts --outDir build/src",
    "dist": "electron-builder -c.extraMetadata.main=build/src/electron/electron.js --publish never"
  },
  "main": "./src/electron/electron.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.1",
    "electron": "^7.1.5",
    "electron-builder": "^21.2.0",
    "electron-reload": "^1.5.0",
    "nodemon": "^2.0.2",
    "wait-on": "^3.3.0"
  }
}
